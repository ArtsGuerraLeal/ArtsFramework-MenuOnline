<!doctype html>
<html lang="en">

<head>

    <title>{% block title %} Successful Payment {% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=yes, shrink-to-fit=no" />


    {% block stylesheets %}
        <link href="{{ asset('fontawesome-free/css/all.min.css') }}" rel="stylesheet" type="text/css">
        <link href="{{ asset('css/main.css') }}" rel="stylesheet">
        <link  href="{{ asset('stripe/normalize.css') }}"rel="stylesheet" >
        <link rel="stylesheet" href="{{ asset('stripe/global.css') }}" >
    {% endblock %}
    <script src="https://js.stripe.com/v3/"></script>
    <script src="{{ asset('js/scriptStripe.js') }}"></script>
</head>

<body class="bg-secondary">

<div class="app-main bg-secondary">

    <div class="app-main__outer">

        <div class="app-main__inner">

            <div class="main-card mb-3 card">

                <div class="card-body">
                    <div id="card-area">
                        <h1>Success</h1>

                    </div>

                </div>
            </div>

        </div>


    </div>
</div>

</body>
{% block javascripts %}

    <script src="{{ asset('jquery/jquery-3.5.1.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('assets/scripts/main.js') }}"></script>


    <script>

    </script>
{% endblock %}
</html>



